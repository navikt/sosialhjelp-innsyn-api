{
  "applicationName": "sosialhjelp-innsyn-api",
  "namespace": "teamdigisos",
  "ingress": [
    "https://sosialhjelp-innsyn-api.nais.oera.no/sosialhjelp/innsyn-api",
    "https://www.nav.no/sosialhjelp/innsyn-api"
  ],
  "envsecret": [
    "redis-password-innsyn-api",
    "xsrf-secret",
    "fiks-integrasjon-secret"
  ],
  "kvPaths": {
    "/var/run/secrets/nais.io/virksomhetssertifikat": "/secret/virksomhetssertifikat/prod/maskinporten",
    "/serviceuser/srvsosial-inn-fss": "/serviceuser/data/prod/srvsosial-inn-fss",
    "/apigw/securitytokenservicetoken": "/apikey/apigw/prod/security-token-service-token/sosialhjelp-innsyn-api",
    "/apigw/norg2": "/apikey/apigw/prod/norg2.api.v1/sosialhjelp-innsyn-api",
    "/apigw/pdl-api": "/apikey/apigw/prod/pdl-api/sosialhjelp-innsyn-api"
  },
  "env": {
    "FIKS_DIGISOS_ENDPOINT_URL": "https://api.fiks.ks.no",
    "FIKS_DOKUMENTLAGER_ENDPOINT_URL": "https://minside.kommune.no",
    "FIKS_SVAR_UT_ENDPOINT_URL": "https://svarut.ks.no",
    "NORG_ENDPOINT_URL": "https://api-gw.oera.no/norg2/api/v1",
    "ID_PORTEN_TOKEN_URL": "https://oidc.difi.no/idporten-oidc-provider/token",
    "ID_PORTEN_CONFIG_URL": "https://oidc.difi.no/idporten-oidc-provider/.well-known/openid-configuration",
    "ID_PORTEN_CLIENT_ID": "bd520e59-6e01-4669-a7c8-f68fa2cac362",
    "ID_PORTEN_SCOPE": "ks:fiks",
    "AZUREAD_LOGINURL": "https://loginservice.nav.no/login",
    "CACHE_TIME_TO_LIVE_SECONDS": "180",
    "REDIS_HOST": "sosialhjelp-innsyn-api-redis.teamdigisos.svc.nais.local",
    "SPRING_PROFILES_ACTIVE": "prod-sbs,log-kibana",
    "PDL_ENDPOINT_URL": "https://api-gw.oera.no/pdl-api/graphql",
    "STS_TOKEN_ENDPOINT_URL": "https://api-gw.oera.no/security-token-service/rest/v1/sts/token",
    "UNLEASH_API_URL": "https://unleash.nais.io/api",
    "UNLEASH_INSTANCE_ID": "prod-sbs",
    "MELDINGER_KOMMUNENUMMER": "3403",
    "SWAGGER_ENABLED": "false",
    "DIALOG_ENDPOINT_URL": "https://www.nav.no/sosialhjelp/meldinger-api/app/api/status",
    "DIALOG_AUDIENCE": "prod-gcp:teamdigisos:sosialhjelp-dialog-api"
  },
  "minReplicas": "2",
  "maxReplicas": "4",
  "cpuLimit": "500m",
  "memoryLimit": "4Gi",
  "cpuRequest": "200m",
  "memoryRequest": "2Gi",
  "prometheusEnabled": true,
  "webProxyEnabled": true,
  "isGcp": false,
  "isLabsGcp": false,
  "vaultEnabled": true,
  "tokenXEnabled": true
}
